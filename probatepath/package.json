{
  "name": "probatepath",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "predev": "node scripts/ensure-prisma-runtime.mjs",
    "dev": "next dev",
    "prebuild": "node scripts/ensure-prisma-runtime.mjs",
    "build": "next build",
    "prestart": "node scripts/ensure-prisma-runtime.mjs",
    "start": "next start",
    "lint": "eslint",
    "typecheck": "tsc --noEmit",
    "postinstall": "node scripts/ensure-prisma-runtime.mjs",
