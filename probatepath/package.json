    "next": "16.0.1",
    "next-auth": "^4.24.13",
    "nodemailer": "^7.0.10",
    "pdf-lib": "^1.17.1",
    "pg": "^8.16.3",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "resend": "^6.4.2",
    "uuid": "^13.0.0",
    "zod": "^4.1.12"
