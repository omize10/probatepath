{
  "installCommand": "cd probatepath && npm install",
  "buildCommand": "cd probatepath && npm run build",
  "outputDirectory": "probatepath/.next",
  "framework": "nextjs",
  "functions": {
    "app/api/forms/generated/**/*.ts": {
      "maxDuration": 30
    }
  },
  "build": {
    "env": {
      "PUPPETEER_SKIP_CHROMIUM_DOWNLOAD": "true",
      "PUPPETEER_EXECUTABLE_PATH": "/usr/bin/chromium"
    }
  }
}
